{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-use-before-define":0,
        "linebreak-style":0,
        "indent":0,
        "react/jsx-indent":0,
        "object-curly-newline":0,
        "react/prop-types": ["error", { "ignore": ["navigation"] }],
        "arrow-body-style": 0
        }
}
